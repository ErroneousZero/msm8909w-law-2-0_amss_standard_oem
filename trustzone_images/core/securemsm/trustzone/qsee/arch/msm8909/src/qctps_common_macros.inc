;The KR_SV_TEST macro writes PVR7F6 register to indicate an SV test is required
;Usage: KR_SV_TEST <source_reg>
;------------------------------------------------------------------------------

       ; Write to PVR7F6 to signal SV test is required
       MACRO
$label   KR_SV_TEST $rx
$label
         MOV     $rx, #0x5356
         WCP15_PVR7F6  $rx
       MEND                 




