;------------------------------------------------------------------------------------------------

;------------------------------------------------------------
; Macro to assert the correct value of register numbers
;------------------------------------------------------------
    MACRO
$label        MACROS_CHECK_REGNO $check,$limit,$instruction 
$label       
    LCLA regno
regno SETA :RCONST:$check
    
    ; Register number should not be more than the limit
    IF  (regno > $limit)
      INFO 1,"Register number of instruction $instruction is greater than $limit"
    ENDIF
    
    ; Register number should not be negative
    IF  (regno < 0)
      INFO 1,"Register number of instruction $instruction is negative"
    ENDIF
    
    MEND
    
;------------------------------------------------------------------------------------------------
; Macros for instruction PLDW in Scorpion MP
;------------------------------------------------------------------------------------------------
;------------------------------------------------------------------------------------------------
; T1 PLDW [<Rn>, #<imm12>]
;------------------------------------------------------------------------------------------------
    MACRO
$label    pldwt1_imm  $rn,$im
$label  
    LCLA rnregno
    LCLA imm12
    LCLS addsub
    LCLA dciforthumb
    LCLA dciforthumb_0
    LCLA dciforthumb_1
rnregno SETA :RCONST:$rn
    MACROS_CHECK_REGNO  $rn, 15, pldwt1_imm
imm12 SETA 0x$im

dciforthumb SETA 0xF8B0F000 :OR: ((rnregno :AND: 0xF) << 16) :OR: (imm12 :AND: 0xFFF)
dciforthumb_0 SETA (dciforthumb :AND: 0x0000FFFF)
dciforthumb_1 SETA ((dciforthumb :AND: 0xFFFF0000) >> 16) 
    DCI 0x00000000 :OR: dciforthumb_1 
    DCI 0x00000000 :OR: dciforthumb_0   
    MEND  
    
;------------------------------------------------------------------------------------------------
; T2 PLDW [<Rn>, #-<imm8>]
;------------------------------------------------------------------------------------------------
    MACRO
$label    pldwt2_imm  $rn,$im
$label  
    LCLA rnregno
    LCLA imm8
    LCLS addsub
    LCLA dciforthumb
    LCLA dciforthumb_0
    LCLA dciforthumb_1
rnregno SETA :RCONST:$rn
    MACROS_CHECK_REGNO  $rn, 15, pldwt2_imm
imm8 SETA 0x$im

dciforthumb SETA 0xF830FC00 :OR: ((rnregno :AND: 0xF) << 16) :OR: (imm8 :AND: 0xFF)
dciforthumb_0 SETA (dciforthumb :AND: 0x0000FFFF)
dciforthumb_1 SETA ((dciforthumb :AND: 0xFFFF0000) >> 16) 
    DCI 0x00000000 :OR: dciforthumb_1 
    DCI 0x00000000 :OR: dciforthumb_0      
    MEND  
    
;------------------------------------------------------------------------------------------------
; A1 PLDW [<Rn>, #+/-<imm12>]
;------------------------------------------------------------------------------------------------
    MACRO
$label    pldw_imm  $rn,$im,$op=+
$label  
    LCLA rnregno
    LCLA imm12
    LCLS addsub
    LCLA U
rnregno SETA :RCONST:$rn
    MACROS_CHECK_REGNO  $rn, 15, pldw_imm
imm12 SETA 0x$im
U SETA 0x1
addsub SETS "$op"  
    IF  ((addsub = "-"))
U SETA 0x0
    ENDIF
    DCI 0xF510F000 :OR: ((rnregno :AND: 0xF) << 16) :OR: (U << 23) :OR: (imm12 :AND: 0xFFF)  
    MEND  
        
;------------------------------------------------------------------------------------------------
; A1 PLDW [<Rn>, +<Rm>]
;------------------------------------------------------------------------------------------------
    MACRO
$label    pldw_reg  $rn, $rm, $op=+ 
$label  
    LCLA rnregno
    LCLA rmregno
    LCLS addsub
    LCLA U    
    LCLA shift_t
rnregno SETA :RCONST:$rn
rmregno SETA :RCONST:$rm
    MACROS_CHECK_REGNO  $rn, 15, pldw_reg
    MACROS_CHECK_REGNO  $rm, 15, pldw_reg
addsub SETS "$op"
U SETA 0x1
shift_t SETA 0x0  
    IF  ((addsub = "-"))
U SETA 0x0
    ENDIF
    DCI 0xF710F000 :OR: ((rnregno :AND: 0xF) << 16) :OR: (U << 23) :OR: (rmregno :AND: 0xF)  
    MEND
    
;------------------------------------------------------------------------------------------------
; A1 PLDW [<Rn>, +<Rm>, {, <shift>]
;------------------------------------------------------------------------------------------------
    MACRO
$label    pldw_reg_$stype  $rn, $rm, $sa=0, $op=+ 
$label  
    LCLA rnregno
    LCLA rmregno
    LCLS addsub
    LCLS shifttype
    LCLA imm
    LCLA U    
    LCLA shift_t
rnregno SETA :RCONST:$rn
rmregno SETA :RCONST:$rm
    MACROS_CHECK_REGNO  $rn, 15, pldw_reg_$stype
    MACROS_CHECK_REGNO  $rm, 15, pldw_reg_$stype
addsub SETS "$op"
shifttype SETS "$stype"
imm SETA 0x$sa
U SETA 0x1
shift_t SETA 0x0  
    IF  ((addsub = "-"))
U SETA 0x0
    ENDIF
    
    IF  ((shifttype="LSL") || (shifttype="lsl"))
shift_t SETA 0x0
    ELIF ((shifttype="LSR") || (shifttype="lsr"))
shift_t SETA 0x1
    ELIF ((shifttype="ASR") || (shifttype="asr"))
shift_t SETA 0x2
    ELIF ((shifttype="RRX") || (shifttype="rrx"))
shift_t SETA 0x3
imm SETA 0x0
    ELIF ((shifttype="ROR") || (shifttype="ror"))
shift_t SETA 0x3
    ENDIF
    DCI 0xF710F000 :OR: ((rnregno :AND: 0xF) << 16) :OR: (U << 23) :OR: (rmregno :AND: 0xF) :OR: (shift_t << 5) :OR: ((imm :AND: 0x1F) << 7)  
    MEND
    
;------------------------------------------------------------------------------------------------
; T1 PLDW [<Rn>, +<Rm>, {, <shift>]
; pldw_reg <rn>, <rm>, <shift> instruction 
;------------------------------------------------------------------------------------------------
    MACRO
$label    pldwt1_reg  $rn, $rm, $imm2=0 
$label  
    LCLA rnregno
    LCLA rmregno
    LCLA imm
    LCLA shift_t
rnregno SETA :RCONST:$rn
rmregno SETA :RCONST:$rm
    MACROS_CHECK_REGNO  $rn, 15, pldwt1_reg
    MACROS_CHECK_REGNO  $rm, 15, pldwt1_reg
imm SETA 0x$imm2
LCLA dciforthumb
LCLA dciforthumb_0
LCLA dciforthumb_1

dciforthumb SETA 0xF830F000 :OR: ((rnregno :AND: 0xF) << 16) :OR: (rmregno :AND: 0xF) :OR: ((imm :AND: 0x1F) << 4)  
dciforthumb_0 SETA (dciforthumb :AND: 0x0000FFFF)
dciforthumb_1 SETA ((dciforthumb :AND: 0xFFFF0000) >> 16) 
    DCI 0x00000000 :OR: dciforthumb_1 
