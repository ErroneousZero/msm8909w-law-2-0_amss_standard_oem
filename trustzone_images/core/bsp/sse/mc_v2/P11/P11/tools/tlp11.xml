<?xml version="1.0" encoding="UTF-8"?>
<!-- 
A template of a trustlet configuration file.
This file contains the trustlet's structure,
with the values to be configured. -->
<!-- 
Elements:
  structure: indication of a trustlet configuration file.
  integer: integer input.
  string: string input.
  boolean: boolean input, value 0/1. 
Attributes:
  name: description of the element.
  length: length of the element in bytes.
Values:
  In order to have an empty string,
 enter as a value \0 .
-->
<structure>
  <integer name="magicStart" length="4">0x31315051</integer>
  <integer name="majorVersion" length="2">1</integer>
  <integer name="minorVersion" length="2">2</integer>
  <string name="label" length="32">\0</string>
  <string name="manufacturerID" length="32">Qualcomm Ltd</string>
  <string name="model" length="16">SSE PKCS#11</string>
  <string name="serialNumber" length="16">050505</string>
  <integer name="flags" length="4">0x00000005</integer>
  <integer name="ulMaxSessionCount" length="4">4</integer>
  <integer name="ulMaxRwSessionCount" length="4">4</integer>
  <integer name="ulMaxPinLen" length="4">12</integer>
  <integer name="ulMinPinLen" length="4">6</integer>
  <integer name="hardwareVersionMajor" length="1">0</integer>
  <integer name="hardwareVersionMinor" length="1">1</integer>
  <integer name="firmwareVersionMajor" length="1">0</integer>
  <integer name="firmwareVersionMinor" length="1">1</integer>
  <integer name="maxAuthSpan" length="4">60000</integer>
  <integer name="maxUsageNum" length="4">100</integer>
  <integer name="maxLocalLoginAttempts" length="4">5</integer>
  <integer name="maxRemoteLoginAttempts" length="4">5</integer>
  <boolean name="allowLocalToken" length="4">1</boolean>
  <integer name="challengeLifespan" length="4">60</integer>
  <boolean name="remoteResetUnlockLocal" length="4">1</boolean>
  <boolean name="unlockAtBoot" length="4">0</boolean>
  <boolean name="localUnlockTime" length="4">0</boolean>
  <boolean name="remoteUnlockTime" length="4">0</boolean>
  <integer name="enabledFunctions" length="4">0x000FFF00</integer>
  <integer name="securityExtensions" length="4">0x00000000</integer>
  <integer name="minIterationsPBKDF2_SK" length="4">100000</integer>
</structure>