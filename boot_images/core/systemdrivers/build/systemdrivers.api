Import('env')

#-------------------------------------------------------------------------------
# Define HWIO API paths
#-------------------------------------------------------------------------------

env.Replace(HWIO_API = [
   "${INC_ROOT}/core/api/systemdrivers/hwio/msm8909",
   "${INC_ROOT}/core/api/systemdrivers/hwio/msm8909/inc/hal",
])

# image specific inc paths
if env.has_key('MODEM_PROC'):
   env.Append(HWIO_API = [
      "${INC_ROOT}/core/api/systemdrivers/hwio/msm8909/inc/proc/modem",
   ])
elif env.has_key('APPS_PROC'):
   env.Append(HWIO_API = [
      "${INC_ROOT}/core/api/systemdrivers/hwio/msm8909/inc/proc/apps",
   ])
elif env.has_key('RPM_IMAGE'):
   env.Append(HWIO_API = [
      "${INC_ROOT}/core/api/systemdrivers/hwio/msm8909/inc/proc/rpm",
   ])
elif env.has_key('WCN_IMAGE'):
   env.Append(HWIO_API = [
      "${INC_ROOT}/core/api/systemdrivers/hwio/msm8909/inc/proc/riva",
   ])
if env.has_key('CORE_SPS'):
   env.Append(HWIO_API = [
      "${INC_ROOT}/core/api/systemdrivers/hwio/msm8909/inc/proc/dsps",
   ])

# plat specific inc paths
if env.has_key('BUILD_BOOT_CHAIN') or env.has_key('BUILD_TOOL_CHAIN') or env.has_key('TZOS_IMAGE') or env.has_key('CORE_SPS') or env.has_key('RPM_IMAGE') or env.has_key('USES_BMPOS_QNK') or env.has_key('USES_REX') or env.has_key('USES_BLAST'):
   env.Append(HWIO_API = [
      "${INC_ROOT}/core/api/systemdrivers/hwio/msm8909/inc/plat/phys",
   ])
else:
   env.Append(HWIO_API = [
      "${INC_ROOT}/core/api/systemdrivers/hwio/msm8909/inc/plat/l4/user",
   ])

#-------------------------------------------------------------------------------
# Publish Public APIs
# These are accessible internal or external to CoreBSP
#-------------------------------------------------------------------------------

env.PublishPublicApi('SYSTEMDRIVERS', [
    "${INC_ROOT}/core/api/systemdrivers",
    "${INC_ROOT}/core/api/systemdrivers/pmic",
    "${HWIO_API}"
])

env.PublishPublicApi('HWIO', env['HWIO_API'])


