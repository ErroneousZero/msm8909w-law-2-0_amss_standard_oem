Import('env')

# Compile for all flavors except for GENS flavor (Wearable).
if env.get('CHIPSET') not in ('msm8909') or 'USES_SENSORS_COMPONENT' in env:
 Return()

if 'USES_FEATURE_RFA_TP_CARDS' in env:
  Return()

if 'USES_GNSS_ONLY_NO_WWAN' in env:
  Return()

env.RequireRestrictedApi([
    'RFDEVICE_INTERFACE',
    'RFA',
    'RFDEVICE',
    ])

env.Replace(RFC_WTR4905_CHINA_CU_4M_ROOT = '${RFC_ROOT}/rf_card/rfc_wtr4905_china_cu_4m');

env.PublishProtectedApi( 'RFC_WTR4905_CHINA_CU_4M_INC', [
    '${RFC_WTR4905_CHINA_CU_4M_ROOT}/gnss/inc',
    '${RFC_WTR4905_CHINA_CU_4M_ROOT}/gsm/inc',
    '${RFC_WTR4905_CHINA_CU_4M_ROOT}/lte/inc',
    '${RFC_WTR4905_CHINA_CU_4M_ROOT}/wcdma/inc',
    ])

env.RequireProtectedApi(['RFC_WTR4905_CHINA_CU_4M_INC'])

env.AddRfCard('RF_HW_WTR4905_CHINA_CU_4M', 'rfc_wtr4905_china_cu_4m_cmn_ag.h', 'rfc_wtr4905_china_cu_4m_cmn_ag')

env.LoadSoftwareUnits( )
