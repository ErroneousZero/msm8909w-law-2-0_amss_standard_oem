Import('env')

# Do not compile for mdm9607 flavor
if env.get('CHIPSET') in ('mdm9607'):
 Return( )

# Do not compile for msm8909 flavor
#if env.get('CHIPSET') in ('msm8909'):
# Return( )
 
if 'USES_FEATURE_RFA_TP_CARDS' in env:
  Return()

if 'USES_GNSS_ONLY_NO_WWAN' in env:
  Return()

env.RequireRestrictedApi([
    'RFDEVICE_INTERFACE',
    'RFA',
    'RFDEVICE',
    ])

env.Replace(RFC_WTR2965_V2_NA_CA_4320_ROOT = '${RFC_ROOT}/rf_card/rfc_wtr2965_v2_na_ca_4320');

env.PublishProtectedApi( 'RFC_WTR2965_V2_NA_CA_4320_INC', [
    '${RFC_WTR2965_V2_NA_CA_4320_ROOT}/cdma/inc',
    '${RFC_WTR2965_V2_NA_CA_4320_ROOT}/gnss/inc',
    '${RFC_WTR2965_V2_NA_CA_4320_ROOT}/gsm/inc',
    '${RFC_WTR2965_V2_NA_CA_4320_ROOT}/lte/inc',
    '${RFC_WTR2965_V2_NA_CA_4320_ROOT}/wcdma/inc',
    ])

env.RequireProtectedApi(['RFC_WTR2965_V2_NA_CA_4320_INC'])

env.AddRfCard('RF_HW_WTR2965_V2_NA_CA_4320', 'rfc_wtr2965_v2_na_ca_4320_cmn_ag.h', 'rfc_wtr2965_v2_na_ca_4320_cmn_ag')

env.LoadSoftwareUnits( )
