<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="@dimen/text_view_layout_height"
        android:id="@+id/textViewListItem"
        android:textSize="@dimen/cellular_list_item_text_size"
        android:singleLine="true"
        android:layout_centerHorizontal="true"
        android:paddingLeft="@dimen/text_view_padding_left"
        android:paddingBottom="@dimen/text_view_padding_bottom"
        android:paddingTop="@dimen/text_view_padding_top" />

</RelativeLayout>
